{
	"manifest_version": 3,
	"name": "Your Extension Name",
	"version": "1.0",
	"description": "Your extension description.",
	"host_permissions": [
		"*://yourwebsite.com/*"
	],
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "icon/icon16.png",
			"48": "icon/icon48.png",
			"32": "icon/icon32.png",
			"128": "icon/icon128.png",
			"256": "icon/icon256.png"
		}
	},
	"icons": {
		"16": "icon/icon16.png",
		"32": "icon/icon32.png",
		"48": "icon/icon48.png",
		"128": "icon/icon128.png",
		"256": "icon/icon256.png"
	},
	"content_scripts": [
		{
			"matches": ["*://yourwebsite.com/*"],
			"js": ["contentscript.js"]
		}
	]
}
